    "type": "concrete5-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Michele Locati",
            "email": "michele@locati.it"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">= 7.2",
        "concrete5/core": ">=8.5",
        "mlocati/nexi-xpay": "^1",
        "mlocati/nexi-xpay-web": "^2"
    },
    "config": {
        "optimize-autoloader" : true,
        "preferred-install": "dist",
        "sort-packages" : true,
        "allow-plugins": {
            "mlocati/composer-patcher": true
